<UserControl x:Class="GiveMeFive.Page.MultipleLuckMemberCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GiveMeFive.Page"
             mc:Ignorable="d" 
             d:DesignHeight="52" d:DesignWidth="158">
    <Grid>

        <StackPanel >
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding MemberName}" VerticalAlignment="Top" Margin="5,2,0,0"/>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding MemberName}" VerticalAlignment="Bottom" Height="32" Margin="5,3,0,0" FontSize="22"/>
        </StackPanel>


    </Grid>
</UserControl>
