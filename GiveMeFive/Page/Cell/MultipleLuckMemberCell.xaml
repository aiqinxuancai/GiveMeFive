<UserControl x:Class="GiveMeFive.Page.Cell.MultipleLuckMemberCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GiveMeFive.Page.Cell"
             mc:Ignorable="d" 
             d:DesignHeight="52" d:DesignWidth="158">
    <Grid>
        <StackPanel Margin="8,8,8,8" >
            <TextBlock x:Name="textDepartment" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding MemberDepartment}" VerticalAlignment="Top" Margin="6,2,0,0" Foreground="#CC000000" FontSize="15"/>
            <TextBlock x:Name="textName" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding MemberName}" VerticalAlignment="Bottom" Height="32" Margin="5,3,0,0" FontSize="30"/>
        </StackPanel>
    </Grid>
</UserControl>
