<UserControl x:Class="GiveMeFive.Page.Cell.MultipleLuckMemberCellVersionTwo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GiveMeFive.Page.Cell"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300">
    <Grid>
        <StackPanel Margin="8,14" Orientation="Horizontal" >
            <TextBlock x:Name="textDepartment" HorizontalAlignment="Center" TextWrapping="Wrap" Text="{Binding MemberDepartment}" VerticalAlignment="Center" Margin="6,2,0,0" Foreground="#CC000000" FontSize="30"/>
            <TextBlock x:Name="textName" HorizontalAlignment="Center" TextWrapping="Wrap" Text="{Binding MemberName}" VerticalAlignment="Center" Margin="10,3,0,0" FontSize="30"/>
        </StackPanel>
    </Grid>
</UserControl>
