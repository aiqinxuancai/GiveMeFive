<UserControl x:Class="GiveMeFive.Page.Cell.OneLuckMemberCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GiveMeFive.Page.Cell"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel >
            <TextBlock x:Name="textDepartment" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding MemberDepartment}" VerticalAlignment="Top" Margin="0" Foreground="#CC000000" FontSize="35"/>
            <TextBlock x:Name="textName" HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding MemberName}" VerticalAlignment="Bottom"  Margin="0,0,0,0" FontSize="70"/>
        </StackPanel>
    </Grid>
</UserControl>
